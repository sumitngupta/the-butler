{
  "name": "the-butler",
  "regions": ["cdg"],
  "build": {
    "env": {
      "PHP_VERSION": "7.3",
      "CECIL_VERSION": "5.14.4",
      "CECIL_BASEURL": "https://the-butler-garth.now.sh/",
      "CECIL_THEME": "garth"
    }
  },
  "builds": [{
    "src": "now-build.sh",
    "use": "@now/static-build",
    "config": { "distDir": "_site" }
  }]
}
